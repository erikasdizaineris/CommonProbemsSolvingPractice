#include <iostream>

using namespace std;

int main(){

int numbArray[] = {1, 2, 3, 4 , 5, 6, 7, 8, 10, 11, 12, 13, 14, 15, 16};

int lengthOfNumbArray = sizeof(numbArray) / sizeof(numbArray[0]) -1;

int lastNumber = numbArray[lengthOfNumbArray];

int numbArraySum = 0;

int numbSum = 0;

for (int i = 0; i < lengthOfNumbArray; i++) {
	numbArraySum += numbArray[i];
}

for (int j = 0; j < lastNumber; j++){
    numbSum += j;
}

if (numbSum != numbArraySum) {

cout << "Missing number is " << numbSum - numbArraySum;

}

else {
	
	cout << "There are no missing numbers. ";
}

return 0;
} 
